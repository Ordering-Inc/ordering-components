{
  "name": "ordering-components",
  "version": "0.0.1",
  "description": "Ordering Internal Dashboard",
  "main": "./_modules/index.js",
  "scripts": {
    "clean": "shx rm -rf _bundles _modules",
    "build": "npm run clean && babel ./src --out-dir ./_modules && babel ./src --out-dir ./_modules/src && babel ./native --out-dir ./_modules/native && webpack --config webpack.prod.js",
    "build-dev": "npx webpack --mode=development",
    "serve": "npx webpack-dev-server --config webpack.dev.js",
    "lint": "standard",
    "test": "cypress open"
  },
  "repository": {
    "url": "git@github.com:Ordering-Inc/ordering-components.git"
  },
  "keywords": [
    "ordering",
    "ordering-components"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@babel/plugin-syntax-jsx": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "babel-loader": "^8.2.2",
    "css-loader": "^3.6.0",
    "cypress": "^4.11.0",
    "eslint-plugin-cypress": "^2.11.2",
    "html-webpack-plugin": "^4.5.1",
    "mini-css-extract-plugin": "^1.3.9",
    "postcss-loader": "^3.0.0",
    "react": "^16.14.0",
    "react-datepicker": "^3.4.0",
    "react-dom": "^16.14.0",
    "react-hook-form": "^6.14.1",
    "react-router-dom": "^5.2.0",
    "sass": "^1.32.4",
    "sass-loader": "^9.0.2",
    "shx": "^0.3.3",
    "standard": "^14.3.4",
    "style-loader": "^1.3.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.1",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@react-native-async-storage/async-storage": "^1.13.2",
    "@sentry/react": "^5.29.2",
    "@sentry/tracing": "^5.29.2",
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.11.0",
