    "prop-types": "^15.7.2",
    "react-google-recaptcha": "latest",
    "react-google-recaptcha-v3": "^1.10.0",
    "socket.io-client": "^2.3.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "cypress run"
    }
  },
  "peerDependencies": {
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/standard/eslintrc.json",
      "plugin:cypress/recommended"
    ]
  },
  "standard": {
    "globals": [
      "describe",
