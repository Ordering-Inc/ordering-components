{
  "name": "ordering-components",
  "version": "0.0.1",
  "description": "Ordering Internal Dashboard",
  "main": "./_modules/index.js",
  "scripts": {
    "clean": "shx rm -rf _bundles _modules",
    "build": "yarn run clean && babel ./src --out-dir ./_modules && babel ./native --out-dir ./_modules/native && webpack --config webpack.prod.js",
    "build-dev": "npx webpack --mode=development",
    "serve": "npx webpack-dev-server --config webpack.dev.js",
    "lint": "standard",
    "test": "cypress open"
  },
  "repository": {
    "url": "git@github.com:Ordering-Inc/ordering-components.git"
  },
  "keywords": [
    "ordering",
    "ordering-components"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-syntax-jsx": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "cypress": "^4.11.0",
    "eslint-plugin-cypress": "^2.11.1",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.3.8",
