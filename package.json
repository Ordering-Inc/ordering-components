    "ordering-api-sdk": "github:Ordering-Inc/ordering-api-sdk#development",
    "prop-types": "^15.7.2",
    "socket.io-client": "^2.4.0"
  },
  "peerDependencies": {
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/standard/eslintrc.json",
      "plugin:cypress/recommended"
    ]
  },
  "standard": {
    "globals": [
      "describe",
      "before",
      "after",
      "beforeEach",
      "afterEach",
      "it",
      "cy",
      "context",
      "assert"
    ]
  }
}
